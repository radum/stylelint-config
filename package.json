{
  "name": "@radum/stylelint-config",
  "version": "6.1.1",
  "description": "@radum Stylelint Shareable Config",
  "keywords": [
    "stylelint",
    "stylelint-config"
  ],
  "author": "Radu Micu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/radum/stylelint-config.git"
  },
  "engines": {
    "node": ">=10"
  },
  "main": "index.js",
  "files": [
    "rules/**/*.js",
    "scss.js",
    "index.js"
  ],
  "scripts": {
    "test": "jest",
    "watch": "jest --watch",
    "lint:js": "eslint --cache --cache-location './.cache/eslint' '**/*.js'",
    "release": "auto shipit"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "postcss-scss": "^4.0.7",
    "stylelint-no-unsupported-browser-features": "^7.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.1.0"
  },
  "devDependencies": {
    "@auto-it/all-contributors": "^11.0.1",
    "@auto-it/first-time-contributor": "^11.0.1",
    "@auto-it/npm": "^11.0.1",
    "@auto-it/released": "^11.0.1",
    "auto": "^11.0.1",
    "eslint": "^8.48.0",
    "jest": "^29.6.4",
    "stylelint": "^15.10.3"
  },
  "peerDependencies": {
    "stylelint": "^15.10.3"
  }
}
